{
    "name": "@nordenwinds/web",
    "type": "module",
    "version": "1.0.0",
    "description": "Frontend of officialsite",
    "scripts": {
        "typegen:contentful": "source .env && cf-content-types-generator --v10 --typeguard --spaceId $CONTENTFUL_SPACE_ID --token $CONTENTFUL_MANAGEMENT_TOKEN --environment $APP_ENV --out src/types/contentful",
        "typegen": "pnpm run typegen:contentful",
        "lint": "eslint src && prettier --check src && astro check && tsc --noEmit",
        "lint:fix": "eslint --fix src && prettier --write src",
        "dev": "astro dev --host",
        "start": "astro dev",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro"
    },
    "author": "Norden Wind Ensemble",
    "license": "MIT",
    "dependencies": {
        "@astrojs/mdx": "^0.19.7",
        "@astrojs/rss": "^2.4.3",
        "@astrojs/sitemap": "^1.4.0",
        "@astrojs/tailwind": "^4.0.0",
        "@contentful/rich-text-html-renderer": "^16.1.0",
        "@contentful/rich-text-plain-text-renderer": "^16.0.5",
        "@contentful/rich-text-types": "^16.2.0",
        "@fontsource/noto-sans-jp": "^5.0.3",
        "@tailwindcss/typography": "^0.5.9",
        "astro": "^2.8.3",
        "contentful": "^10.3.1",
        "tailwindcss": "^3.3.3"
    },
    "devDependencies": {
        "@nordenwinds/eslint-config": "workspace:^",
        "@nordenwinds/prettier-config": "workspace:^",
        "cf-content-types-generator": "^2.12.2",
        "eslint-plugin-astro": "^0.26.2",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-tailwindcss": "^3.13.0",
        "prettier-plugin-astro": "^0.8.1",
        "prettier-plugin-tailwindcss": "^0.2.8"
    },
    "prettier": "@nordenwinds/prettier-config"
}
