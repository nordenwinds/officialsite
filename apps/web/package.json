{
  "name": "@nordenwinds/web",
  "type": "module",
  "version": "1.0.0",
  "description": "Frontend of officialsite",
  "scripts": {
    "typegen:contentful": "source .env && cf-content-types-generator --v10 --jsdoc --typeguard --spaceId $CONTENTFUL_SPACE_ID --token $CONTENTFUL_MANAGEMENT_TOKEN --environment $APP_ENV --out src/types/contentful",
    "typegen": "npm run typegen:contentful",
    "lint": "eslint src && prettier --check src && astro check && tsc --noEmit",
    "lint:fix": "eslint --fix src && prettier --write src",
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "author": "Norden Wind Ensemble",
  "license": "MIT",
  "dependencies": {
    "@astrojs/mdx": "^0.19.0",
    "@astrojs/rss": "^2.4.0",
    "@astrojs/sitemap": "^1.2.2",
    "@astrojs/tailwind": "^3.1.1",
    "@contentful/rich-text-html-renderer": "^16.0.4",
    "@fontsource/noto-sans-jp": "^4.5.12",
    "astro": "^2.3.2",
    "contentful": "^10.2.0",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@nordenwinds/eslint-config": "^1.0.0",
    "@nordenwinds/prettier-config": "^1.0.0",
    "cf-content-types-generator": "^2.12.2",
    "eslint-plugin-astro": "^0.26.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "prettier-plugin-astro": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.2.8"
  },
  "prettier": "@nordenwinds/prettier-config"
}
